            <string>/// Load the high scores

// Destroy all other objLeaderboardLoaders
var me = id;
with objLeaderboardLoader {
    if id != me then instance_destroy();
}

// Slow animation speed
self.image_speed = 30/room_speed;
// Position the loader in the center of the room
self.x = room_width / 2;
self.y = room_height / 2;
var leaderboard = instance_find(objLeaderboard,0);

// Clear errors from the leaderboard
leaderboard.error = "";
// Remove the existing scores
// from the leaderboard
if ds_exists(leaderboard.scores,ds_type_list) {
    ds_list_destroy(leaderboard.scores);
    leaderboard.scores = noone;
}

// Request the scores from the Flox server
flox_leaderboard_load(leaderboard.leaderboardId,leaderboard.scope,
    scrOnLeaderboardComplete,scrOnLeaderboardError);
</string>
